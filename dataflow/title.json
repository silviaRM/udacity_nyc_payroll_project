{
	"name": "title",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "TitleMaster",
						"type": "DatasetReference"
					},
					"name": "title"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "TitleMaster_db",
						"type": "DatasetReference"
					},
					"name": "sink4"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          TitleCode as string,",
				"          TitleDescription as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> title",
				"title sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          TitleCode as string,",
				"          TitleDescription as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> sink4"
			]
		}
	}
}